<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_OUT">
      <PatternLayout pattern="%d %-5p %-20.20C{1} %m%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="CONSOLE" />
      <Logger name="org.folio.rest.persist.PostgresClient" level="warn" />
    </Root>
  </Loggers>
</Configuration>
